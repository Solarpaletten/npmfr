{
  "name": "solar-nbk",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "nbk",
    "frontend"
  ],
  "scripts": {
    "start": "cd nbk && npm start",
    "build": "cd nbk && npm install",
    "dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd nbk && npm start",
    "start:frontend": "cd frontend && npm start"
  },
  "dependencies": {
    "concurrently": "^8.2.0"
  }
} 